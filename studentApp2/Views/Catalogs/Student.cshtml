@using System.Web.Mvc.Html;
@model studentApp2.Models.CatalogStudentViewModel
@{
    ViewBag.Title = "Catalog";
}
<h1>Catalog</h1>

<div class="jumbotron">
    <table class="table">
        <tr>
            <th>Materie</th>
            <th>Data</th>
            <th>Nota</th>
        </tr>
        @foreach (var grade in Model.grades)
        {
            <tr>

                <td>
                    @grade.CourseName
                </td>
                <td>
                   @grade.GradeDate.ToString().Split(' ')[0]
                </td>
               <td>
                   @grade.Grade
               </td>
            </tr>
}
    </table>
</div>